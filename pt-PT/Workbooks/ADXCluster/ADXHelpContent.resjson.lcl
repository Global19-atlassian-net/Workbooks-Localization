<LCX SchemaVersion="6.0" Name="S:\output\loc\ADXCluster\ADXHelpContent.resjson" PsrId="306" FileType="1" SrcCul="en-US" TgtCul="pt-PT" xmlns="http://schemas.microsoft.com/locstudio/2006/6/lcx">
